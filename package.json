{
	"name": "ramadoka.com",
	"version": "0.1.0",
	"type": "commonjs",
	"engines": {
		"node": "16"
	},
	"private": true,
	"scripts": {
		".prepare": "husky install",
		"pre-commit-lint": "lint-staged",
