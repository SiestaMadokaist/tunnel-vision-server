		"body-parser": "^1.20.0",
		"cors": "^2.8.5",
		"dotenv": "^16.0.0",
		"dynamoose": "^3.0.0-beta.1",
		"express": "^4.17.3",
		"sqs-consumer": "^5.7.0"
	},
	"devDependencies": {
		"@serverless/typescript": "^3.0.0",
		"@types/aws-lambda": "^8.10.71",
		"@types/body-parser": "^1.19.2",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.14",
		"@types/node": "^14.14.25",
		"aws-sdk": "^2.1222.0",
		"esbuild": "^0.14.11",
		"json-schema-to-ts": "^1.5.0",
		"serverless": "3.20.0",
